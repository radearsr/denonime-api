{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "quotes": ["error", "double"],
        "linebreak-style": ["off", "windows"],
        "no-console": "off",
        "no-underscore-dangle": "off",
        "no-await-in-loop": "off",
        "radix": "off",
        "no-unused-vars": ["error", { "argsIgnorePattern": "next" }]
    }
}
