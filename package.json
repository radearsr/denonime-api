  "main": "index.js",
  "scripts": {
    "build": "npx prisma db pull --schema=./src/prisma/schema.prisma && npx prisma generate --schema=./src/prisma/schema.prisma",
    "start": "node ./src/index.js",
    "dev": "nodemon ./src/index.js",
    "db-studio": "npx prisma studio --schema=./src/prisma/schema.prisma",
    "db-generate": "npx prisma generate --schema=./src/prisma/schema.prisma",
    "db-push": "npx prisma db push --schema=./src/prisma/schema.prisma",
    "vercel-build": "npx prisma db push --schema=./src/prisma/schema.prisma --accept-data-loss && npx prisma generate --schema=./src/prisma/schema.prisma"
  },
